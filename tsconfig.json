{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es2017",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "paths": {
      "y/interceptors": [
        "libs/interceptors/src"
      ],
      "y/interceptors/*": [
        "libs/interceptors/src/*"
      ],
      "y/shop-interceptor": [
        "libs/shop-interceptor/src"
      ],
      "y/shop-interceptor/*": [
        "libs/shop-interceptor/src/*"
      ]
    }
  },
  "exclude": [
    "node_modules",
    "dist"
  ]
}